/* 水平间距 */
swiper {
  height: 330rpx;
}
swiper .swiper-item,
swiper image {
  width: 100%;
  height: 100%;
}
.nav-list {
  display: flex;
  justify-content: space-around;
  margin: 15px 0;
}
.nav-list .nav-img {
  width: 128rpx;
  height: 140rpx;
}
.nav {
  display: flex;
}
.nav .nav1 {
  height: 50px;
  width: 50px;
  background-color: pink;
}
.nav .nav2 {
  height: 50px;
  width: 50px;
  background-color: skyblue;
}
.floor-list {
  padding: 0 5px;
}
.floor-list .floor-list-item .floor-title {
  height: 60rpx;
  width: 100%;
}
.floor-list .floor-list-item .floor-img-box {
  display: flex;
}
.floor-list .floor-list-item .floor-img-box .img-left {
  width: 232rpx;
}
.floor-list .floor-list-item .floor-img-box .right-img-box {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}

